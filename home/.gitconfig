[user]
    name = Marco Chomut
    email = marco.chomut@gmail.com
[github]
    user = Pewpewarrows
[color]
    ui = auto
    diff = auto
    status = auto
    branch = auto
    interactive = auto
[core]
    pager = less -R
    excludesfile = /Users/Marco/.gitignore_global
[alias]
    st = status
    ci = commit
    ca = commit -a -m
    co = checkout
    br = branch
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
    wtf = !git-wtf
    diff = diff --word-diff --ignore-space-at-eol
    unstage = reset HEAD
[merge]
    conflictstyle = diff3
[credential]
    helper = osxkeychain
